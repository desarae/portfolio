[id*=&#39;toggle&#39;] .container, [class*=&#39;toggle&#39;] .container  {
    transition: margin 300ms cubic-bezier(0.17, 0.04, 0.03, 0.94);
}
[id*=&#39;toggle&#39;] {
    visibility: hidden;
    appearance:none;
    cursor:pointer;
    left:-100%;
    top:-100%;
}
[id*=&#39;toggle&#39;] + label div {
    cursor:pointer;
    text-align: left;
    -webkit-font-smoothing: antialiased;
    cursor: pointer;
    transition:all 500ms ease;
	
}
[id*=&#39;toggle&#39;] + label div:before {
    content:&#39;\002b&#39;; /*OPEN*/
    text-align: left;
    float: left;
}
[id*=&#39;toggle&#39;]:checked + label div:before {
    content:&#39;\2212&#39;; /*close*/
    text-align: left;
    float: left;
}

.nav [id*=&#39;toggle&#39;] + label div:before {
    content:&#39; &#39;; /*OPEN*/
}
.nav [id*=&#39;toggle&#39;]:checked + label div:before {
    content:&#39; &#39;; /*close*/
}

[id*=&#39;toggle&#39;]:checked ~ .toggle-inside {
    display: block;
}
[id*=&#39;toggle&#39;]:checked ~ .container {
    display: none;
}
